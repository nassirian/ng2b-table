{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "sourceMap": true,
    "inlineSources": false,
    "declaration": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "stripInternal": true,
    "skipLibCheck": true,
    "outDir": "./dist"

  },
  "exclude": [
    "node_modules",
    "dist",
    "**/*.spec.ts"
  ]
}
